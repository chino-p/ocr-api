<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.trilight.ocr.mapper.DeliveryOrderMapper">

    <select id="pageDeliveryOrder" resultType="com.trilight.ocr.model.dto.purchase.DeliveryOrderDTO">
        SELECT * FROM delivery_header
        <where>
            <if test="deliveryOrderDTO.supplierCode != null and vatInvoiceDTO.supplierCode != ''">
                AND supplier_code LIKE CONCAT('%', #{deliveryOrderDTO.supplierCode}, '%')
            </if>
        </where>
        ORDER BY id DESC
    </select>
</mapper>
